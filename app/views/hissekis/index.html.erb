<p id="notice"><%= notice %></p>

<h1>Hissekis</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hissekis.each do |hisseki| %>
      <tr>
        <td><%= hisseki.id %></td>
        <td><%= hisseki.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>


